{
  "name": "nodejs-docker",
  "description": "This repository contains the Dockerfile used to build the Node.js runtime image used by Google App Engine",
  "scripts": {
    "test": "npm run test-base && npm run test-builder",
    "test-base": "npm run build && npm run prepare-test && mocha --timeout 25000 dist/test/base/**/*.js",
    "prepare-test": "find test/base/* -type d -maxdepth 0 -exec docker build -t {} {} \\;",
    "build": "docker build -t test/nodejs base",
    "pretest-builder": "./transpile.sh",
    "test-builder": "mocha --timeout 4000 dist/test/builder/**/*.js"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.5.29",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/node-uuid": "0.0.28",
    "@types/request": "0.0.40",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-typescript": "^3.1.5",
    "mocha": "^2.5.3",
    "node-uuid": "^1.4.7",
    "request": "^2.69.0",
    "source-map-support": "^0.4.11",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "dedent-js": "^1.0.1",
    "js-yaml": "^3.8.1",
    "shell-escape": "^0.2.0"
  }
}
